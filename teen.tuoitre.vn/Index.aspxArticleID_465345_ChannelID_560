<script language="javascript" type="text/javascript">  
    // Set ReferUrl
    var hidReferrer = document.getElementById("_ctl0_ContactInline1_hidReferrer");
    hidReferrer.value = window.parent.location.href;
 	
 	// Add file
	function addFile()
	{
	    var addmoreFile = document.getElementById('addmoreFile');
	    if (addmoreFile.style.display == "none")
            addmoreFile.style.display = "block";
        else
            addmoreFile.style.display = "none";
	}
	
	function OutlnkButton(e)
    {
        e.className='lnkButton';    
    }
    
    function OverlnkButton(e)
    {
        e.className='lnkButtonOver';    
    }
    
	function OutlnkButton_reset(e)
    {
        e.className='lnkButton_reset';    
    }
    
    function OverlnkButton_reset(e)
    {
        e.className='lnkButtonOver_reset';    
    }
</script>

  
