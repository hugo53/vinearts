 <script language="javascript" type="text/javascript">
        var cookieUser = new CJL_CookieUtil("TTO_TEEN", 60 * 24 * 7, "/", "");        
        //Initialize varible Cookies
        if (!cookieUser.cookieExists()) {
            cookieUser.setSubValue("keyCookie", "clientCookie");
        }

        var theme = 'App_Themes/Green/GreenStyle.css';

        //Get value current theme from cookie
        if (!cookieUser.getSubValue('theme')) {
            cookieUser.setSubValue('theme', 'App_Themes/Green/GreenStyle.css');
        }
        else {
            theme = cookieUser.getSubValue('theme');
        }

        var css = document.createElement('link');
        css.rel = 'stylesheet';
        css.type = 'text/css';
        css.href = theme;
        document.body.appendChild(css);
        if (debug) {
            monitor(dynStyles, theme);
        }
        cookieUser.setSubValue('theme', theme);

        function setHomepage() {
            if (document.all) {
                document.body.style.behavior = 'url(#default#homepage)';
                document.body.setHomePage('http://teen.tuoitre.com.vn/Index.aspx');

            }
            else if (window.sidebar) {
                if (window.netscape) {
                    try {
                        netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
                    }
                    catch (e) {
                    }
                }
                var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
                prefs.setCharPref('browser.startup.homepage', 'http://teen.tuoitre.com.vn/Index.aspx');
            }
        } 
    </script>
   
