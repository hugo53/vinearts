    <script language="javascript" type="text/javascript">
        function ReloadPage() {
            setTimeout("ReloadPage()", 600000);
            if ((GetPostVariable("ArticleID", "-1") != "-1") || (GetPostVariable("ContactID", "-1") != "-1") || (GetPostVariable("InterviewID", "-1") != "-1")) {
                return;
            }
            else {
                location.reload();
            }
        }
        var channelID = GetPostVariable("ChannelID", -1);
        if (channelID != -1) {
            document.getElementById('DivNSSPlcCenter').style.display = "block";
            document.getElementById('DivNSSContent').style.display = "none";
        }
        else {
            document.getElementById('DivNSSPlcCenter').style.display = "none";
            document.getElementById('DivNSSContent').style.display = "block";
        } 
                           
    </script>

