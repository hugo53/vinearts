<script language="javascript" type="text/javascript">
    
    document.getElementById('New').innerHTML = "Tin mới cập nhật";

    function TabMostpopular(showID, hideID1, hideID2, InnerTitle) {
        switch (InnerTitle) {
            case "1":
                document.getElementById('New').innerHTML = "Tin mới cập nhật";
                document.getElementById('Hot').innerHTML = "Tin nổi bật";
                document.getElementById('MostView').innerHTML = "Tin đọc nhiều nhất";
                break;
            case "2":
                document.getElementById('New').innerHTML = "Tin nổi bật";
                document.getElementById('MostView').innerHTML = "Tin mới cập nhật";
                document.getElementById('Hot').innerHTML = "Tin đọc nhiều nhất";
                break;
            case "3":
                document.getElementById('New').innerHTML = "Tin đọc nhiều nhất";
                document.getElementById('MostView').innerHTML = "Tin mới cập nhật";
                document.getElementById('Hot').innerHTML = "Tin nổi bật";
                break;
            default:
                document.getElementById('New').innerHTML = "Tin mới cập nhật";
                break;
        }

        // show elment
        document.getElementById(showID).className = "txt_f26522_ml_bold";
        document.getElementById(showID + "Content").style.display = "block";
        // hide element
        document.getElementById(hideID1).className = "txt_black_m_bold";
        document.getElementById(hideID1 + "Content").style.display = "none";

        document.getElementById(hideID2).className = "txt_black_m_bold";
        document.getElementById(hideID2 + "Content").style.display = "none";
    }        
</script>


                                    
