<script language="javascript" type="text/javascript">
    function LoadData() {
        var cbovalue = jQuery("#cboLocattion").val();
        jQuery.ajax({
            type: "POST",
            url: 'Ajax/Weather.aspx?&param=' + cbovalue,
            data: "",
            success: function(req) { jQuery('#divData').html(req); }
        });
    }
    LoadData();
</script>
