                <input type="text" id="txtKeyword" class="inputSearch floatLeft" onkeypress="return trapEnterKey('chkYahoo',this.value,event);" style="width: 114px; height: 18px;"
                    value="Tìm trên Tuổi Trẻ" onblur="if (this.value=='') this.value='Tìm trên Tuổi Trẻ'; else if(document.getElementById('chkYahoo').checked) this.value=='Tìm trên Yahoo';" 
            onfocus="if (this.value=='Tìm trên Tuổi Trẻ'||this.value=='Tìm trên Yahoo') this.value='';"  name="SearchQuery" />
