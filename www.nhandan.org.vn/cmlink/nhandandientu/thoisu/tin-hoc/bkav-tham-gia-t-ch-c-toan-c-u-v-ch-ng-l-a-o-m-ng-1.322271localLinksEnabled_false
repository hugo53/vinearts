function renderCaptcha276590() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-276590");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-276590');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-276590" />');
        jQuery("#captcha_image-276590").click(renderCaptcha276590);
    }
}

jQuery().ready(function() { 
            
    
