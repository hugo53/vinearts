function renderCaptcha274924() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-274924");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-274924');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-274924" />');
        jQuery("#captcha_image-274924").click(renderCaptcha274924);
    }
}

