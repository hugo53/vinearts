function renderCaptcha277669() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-277669");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-277669');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-277669" />');
        jQuery("#captcha_image-277669").click(renderCaptcha277669);
    }
}

