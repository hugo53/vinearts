function sendMail(){
$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=Thể thao Việt Nam tiếp tục giành thêm chín Huy chương vàng&url=http://www.nhandan.org.vn/cmlink/nhandandientu/thoisu/the-thao/seagames26/tintuc-sukien/th-thao-vi-t-nam-ti-p-t-c-gianh-them-chin-huy-ch-ng-vang-1.322227&locale=vi_VN");
