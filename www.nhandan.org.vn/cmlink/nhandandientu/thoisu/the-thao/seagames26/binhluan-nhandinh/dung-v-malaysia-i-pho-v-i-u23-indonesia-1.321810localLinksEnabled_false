function renderCaptcha276045() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-276045");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-276045');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-276045" />');
        jQuery("#captcha_image-276045").click(renderCaptcha276045);
    }
}

