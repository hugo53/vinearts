JNewQuery().ready(function() { 
    JNewQuery("#email_this_article-123").click(function() {
        sendMail();
    });
	
});


function sendMail(){
$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=Dùng "võ" Malaysia đối phó với U23 Indonesia ?&url=http://www.nhandan.org.vn/cmlink/nhandandientu/thoisu/the-thao/seagames26/binhluan-nhandinh/dung-v-malaysia-i-pho-v-i-u23-indonesia-1.321810&locale=vi_VN");
return false;
}

JNewQuery.fn.sendMail = function(url)
    {
    if(url == null) 
    return;
    
    window.open(url,"_blank","Send article through email","location=0,status=0,scrollbars=0,width=300,height=400"); 
    }
</script> 

<script type="text/javascript">
var resetRand=false;
var first_time_to_render_captcha_123 = true;
      var captchaLink="";
      function firstTimeCaptcha123() {
        if (first_time_to_render_captcha_123) {
            renderCaptcha123();
            JNewQuery("#email_container-123").show();
        }
      }
      
    function renderCaptcha123() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = JNewQuery("#captcha_image-123");
     
     if(resetRand){
         randNum = Math.ceil(Math.random()*10000000);
     }
     
     captchaLink=window.location.protocol+"//"+window.location.host+"/captcha?" + randNum;
     
     if (captchaImg.length > 0) {
        captchaImg.attr("src",captchaLink); 
     } else {
        var captcha = JNewQuery('#captcha_container-123');
        captcha.html('<img src="' +captchaLink+'" class="captcha_img" id="captcha_image-123" name="picture"/>');
     }
     var img = new Image();
     img.src=captchaLink;
     temp="document['picture'].src =img.src;";
     eval( temp);
     resetRand=true;
    
}



  	var vUni = new vietUni();
