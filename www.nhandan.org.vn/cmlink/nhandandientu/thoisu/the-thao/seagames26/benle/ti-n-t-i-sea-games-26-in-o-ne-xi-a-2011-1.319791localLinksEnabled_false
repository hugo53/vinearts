function renderCaptcha273727() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-273727");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-273727');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-273727" />');
        jQuery("#captcha_image-273727").click(renderCaptcha273727);
    }
}

