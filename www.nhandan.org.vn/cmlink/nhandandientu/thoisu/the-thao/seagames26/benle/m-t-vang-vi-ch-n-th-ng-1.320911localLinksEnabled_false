function sendMail(){
$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=Mất vàng vì chấn thương&url=http://www.nhandan.org.vn/cmlink/nhandandientu/thoisu/the-thao/seagames26/benle/m-t-vang-vi-ch-n-th-ng-1.320911&locale=vi_VN");
