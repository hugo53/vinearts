function renderCaptcha274615() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-274615");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-274615');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-274615" />');
        jQuery("#captcha_image-274615").click(renderCaptcha274615);
    }
}

