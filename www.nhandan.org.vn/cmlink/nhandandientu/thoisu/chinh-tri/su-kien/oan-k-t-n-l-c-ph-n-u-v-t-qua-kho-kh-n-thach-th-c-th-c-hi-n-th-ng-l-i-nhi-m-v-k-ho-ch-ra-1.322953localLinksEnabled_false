function renderCaptcha277379() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-277379");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-277379');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-277379" />');
        jQuery("#captcha_image-277379").click(renderCaptcha277379);
    }
}

