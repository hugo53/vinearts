function sendMail(){
$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=Ðoàn kết, nỗ lực phấn đấu, vượt qua khó khăn, thách thức, thực hiện thắng lợi nhiệm vụ kế hoạch đã đề ra (*)&url=http://www.nhandan.org.vn/cmlink/nhandandientu/thoisu/chinh-tri/su-kien/oan-k-t-n-l-c-ph-n-u-v-t-qua-kho-kh-n-thach-th-c-th-c-hi-n-th-ng-l-i-nhi-m-v-k-ho-ch-ra-1.322953&locale=vi_VN");
