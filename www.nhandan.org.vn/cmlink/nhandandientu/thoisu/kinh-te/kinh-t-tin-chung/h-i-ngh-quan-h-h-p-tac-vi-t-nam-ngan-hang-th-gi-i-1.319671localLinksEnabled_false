		      }
    };

function renderCaptcha273571() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-273571");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-273571');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-273571" />');
        jQuery("#captcha_image-273571").click(renderCaptcha273571);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-273571").click(renderCaptcha273571);
    
    jQuery("#comment_this_article-273571").click(function() {
        firstTimeCaptcha273571();
        jQuery("#comments_container-273571").slideToggle("fast");
        jQuery("#comment_author-273571").focus();
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-273571").validate(validation);
  });
  
