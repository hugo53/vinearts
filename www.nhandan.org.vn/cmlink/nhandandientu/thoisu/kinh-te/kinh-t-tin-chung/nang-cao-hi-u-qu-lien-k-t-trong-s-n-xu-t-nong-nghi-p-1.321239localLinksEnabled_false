		      }
    };

function renderCaptcha275362() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-275362");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-275362');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-275362" />');
        jQuery("#captcha_image-275362").click(renderCaptcha275362);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-275362").click(renderCaptcha275362);
    
    jQuery("#comment_this_article-275362").click(function() {
        firstTimeCaptcha275362();
        jQuery("#comments_container-275362").slideToggle("fast");
        jQuery("#comment_author-275362").focus();
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-275362").validate(validation);
  });
  
