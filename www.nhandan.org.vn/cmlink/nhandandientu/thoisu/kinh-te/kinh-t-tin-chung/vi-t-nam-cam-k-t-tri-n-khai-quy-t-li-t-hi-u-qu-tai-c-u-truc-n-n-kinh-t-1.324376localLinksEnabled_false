function renderCaptcha279076() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-279076");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-279076');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-279076" />');
        jQuery("#captcha_image-279076").click(renderCaptcha279076);
    }
}

