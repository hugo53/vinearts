<div id="modalPage">
    <div class="modalBackground" id="modalBackground"></div>
    <div class="modalContainer" id="modalContainer">
        <div class="modal">
            <div class="modalTop" style="display:none;">
                <a href="javascript:hideModal('modalPage')">[Đóng lại]</a>
