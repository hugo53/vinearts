		      }
    };

function renderCaptcha276129() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-276129");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-276129');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-276129" />');
        jQuery("#captcha_image-276129").click(renderCaptcha276129);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-276129").click(renderCaptcha276129);
    
    jQuery("#comment_this_article-276129").click(function() {
        firstTimeCaptcha276129();
        jQuery("#comments_container-276129").slideToggle("fast");
        jQuery("#comment_author-276129").focus();
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-276129").validate(validation);
  });
  
