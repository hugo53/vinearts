function renderCaptcha275544() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-275544");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-275544');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-275544" />');
        jQuery("#captcha_image-275544").click(renderCaptcha275544);
    }
}

