function renderCaptcha276357() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-276357");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-276357');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-276357" />');
        jQuery("#captcha_image-276357").click(renderCaptcha276357);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-276357").click(renderCaptcha276357);
    
    jQuery("#comment_this_article-276357").click(function() {
        firstTimeCaptcha276357();
        jQuery("#comments_container-276357").slideToggle("fast");
        jQuery("#comment_author-276357").focus();
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-276357").validate(validation);
  });
  
