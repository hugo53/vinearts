		      }
    };

function renderCaptcha274777() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-274777");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-274777');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-274777" />');
        jQuery("#captcha_image-274777").click(renderCaptcha274777);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-274777").click(renderCaptcha274777);
    
    jQuery("#comment_this_article-274777").click(function() {
        firstTimeCaptcha274777();
        jQuery("#comments_container-274777").slideToggle("fast");
        jQuery("#comment_author-274777").focus();
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-274777").validate(validation);
  });
  
