<script type="text/javascript">
cols = document.getElementsByTagName('div');
function Expand(id, count)
{
    if (count == '0')
    {
        //location.href = 'vinabooklist.aspx?catid=' + id;
	location.href = 'booklist.aspx?catid=' + id;
        return;
    }
    // hide all MenuItem
    iLength = cols.length;
    for (i = 0; i < iLength; i++)
    {
        if (cols[i].id.indexOf('Item') != -1)
        {
            cols[i].style.display = 'none';
        }
    }
    if (document.getElementById('Item' + id))
        document.getElementById('Item' + id).style.display = 'block';
}
</script>
