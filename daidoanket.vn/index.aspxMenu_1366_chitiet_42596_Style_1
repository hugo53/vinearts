<script language="javascript" type="text/javascript" src="Scripts/PopCalendar.js"></script>
<script type="text/javascript">
	var lastdiv = null;
	function getLastest(Menu)
	{
		var dvShow = document.getElementById('dv' + Menu);
        dvShow.innerHTML = '<img src=\"/Images/Loading.gif\" border=0 />';
        DHBK.wucTintuc.getNews(Menu,getLastest_CallBack,dvShow);
	
	}	
	
	
	function getLastest_CallBack(response) 
    {
            if (response.error != null) 
            {
                return;
            }
            else
            {   
                if (response.value != null && response.value != '')
                response.context.innerHTML = response.value;
            }
        
    }
	
	
</script>
