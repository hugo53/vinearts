<div class="fl ctdt"><div class="fl bortnd"></div>
	<script>
	writeSociable('http://seagames.vnexpress.net/tin/2011/11/hlv-goetz-ban-thang-da-giup-u23-viet-nam-tu-tin-hon/','HLV+Goetz:+Bàn+thắng+đã+giúp+U23+Việt+Nam+tự+tin+hơn','HLV+Goetz:+Bàn+thắng+đã+giúp+U23+Việt+Nam+tự+tin+hơn','sociable',1002160921);
