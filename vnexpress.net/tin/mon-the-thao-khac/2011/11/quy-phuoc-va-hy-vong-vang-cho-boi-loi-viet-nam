	<script>
	writeSociable('http://seagames.vnexpress.net/tin/mon-the-thao-khac/2011/11/quy-phuoc-va-hy-vong-vang-cho-boi-loi-viet-nam/','Quý+Phước+và+hy+vọng+vàng+cho+bơi+lội+Việt+Nam','Quý+Phước+và+hy+vọng+vàng+cho+bơi+lội+Việt+Nam','sociable',1000682527);
