	<script>
	writeSociable('http://seagames.vnexpress.net/tin/bong-da/2011/10/u23-lao-thua-u23-malaysia-1-4-trong-tran-giao-huu/','U23+Lào+thua+U23+Malaysia+1-4+trong+trận+giao+hữu','U23+Lào+thua+U23+Malaysia+1-4+trong+trận+giao+hữu','sociable',1000541532);
