	<script>
	writeSociable('http://seagames.vnexpress.net/tin/bong-da/2011/11/truc-tiep-u23-viet-nam-1-1-u23-philippines-hiep-2/','Trực+tiếp+U23+Việt+Nam+1-1+U23+Philippines+(Hiệp+2)','Trực+tiếp+U23+Việt+Nam+1-1+U23+Philippines+(Hiệp+2)','sociable',1000718934);
