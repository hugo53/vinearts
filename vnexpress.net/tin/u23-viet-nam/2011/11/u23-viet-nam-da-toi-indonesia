	<script>
	writeSociable('http://seagames.vnexpress.net/tin/u23-viet-nam/2011/11/u23-viet-nam-da-toi-indonesia/','U23+Việt+Nam+đã+tới+Indonesia','U23+Việt+Nam+đã+tới+Indonesia','sociable',1000541585);
