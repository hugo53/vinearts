	<script>
	writeSociable('http://seagames.vnexpress.net/tin/u23-viet-nam/2011/11/u23-viet-nam-xoc-day-cho-tran-dau-cuoi/','U23+Việt+Nam+xốc+dậy+cho+trận+đấu+cuối','U23+Việt+Nam+xốc+dậy+cho+trận+đấu+cuối','sociable',1002162204);
