<div class="fl ctdt"><div class="fl bortnd"></div>
	<script>
	writeSociable('http://seagames.vnexpress.net/tin/u23-viet-nam/2011/11/u23-viet-nam-bi-u23-myanmar-cam-chan/','U23+Việt+Nam+bị+U23+Myanmar+cầm+chân','U23+Việt+Nam+bị+U23+Myanmar+cầm+chân','sociable',1001399699);
