	<script>
	writeSociable('http://seagames.vnexpress.net/tin/mon-khac/2011/11/ngay-12-11-doi-tuyen-the-duc-nam-chot-hc-vang-thu-sau/','Ngày+12/11:+Đội+tuyển+thể+dục+nam+chốt+HC+vàng+thứ+sáu','Ngày+12/11:+Đội+tuyển+thể+dục+nam+chốt+HC+vàng+thứ+sáu','sociable',1002160893);
