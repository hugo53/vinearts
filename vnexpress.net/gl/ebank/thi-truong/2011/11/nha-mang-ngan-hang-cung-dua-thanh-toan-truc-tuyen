<script type="text/javascript" language="javascript">
	function switchToolsTab(i){		
		for(var j=1; j<4; j++){
			if(i==j){				
				gmobj('toolstab' + j).className = 'tagAct';				
			}
			else{
				gmobj('toolstab' + j).className = 'tagNor';
			}
		}
	}
	
	function ShowNotComplete(){
		gmobj('dToolsCom').style.display = 'none';
		gmobj('dNotComplete').style.display = '';
		gmobj('dByTicket').style.display = 'none';
		gmobj('dIBanking').style.display = 'none';
	}
	
	function ShowCommunication(){
		gmobj('dToolsCom').style.display = '';
		gmobj('dNotComplete').style.display = 'none';
		gmobj('dByTicket').style.display = 'none';
		gmobj('dIBanking').style.display = 'none';
	}
	
	function ShowInternetBanking(){
		gmobj('dToolsCom').style.display = 'none';
		gmobj('dNotComplete').style.display = 'none';
		gmobj('dByTicket').style.display = 'none';
		gmobj('dIBanking').style.display = '';
	}
	
	function ShowByTicket(){
		gmobj('dToolsCom').style.display = 'none';
		gmobj('dNotComplete').style.display = 'none';
		gmobj('dByTicket').style.display = '';
		gmobj('dIBanking').style.display = 'none';
	}
</script>					
