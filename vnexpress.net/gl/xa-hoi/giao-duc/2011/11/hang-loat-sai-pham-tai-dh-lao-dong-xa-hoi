<script language="JavaScript">
function ViewTop(sView)
{
	switch (sView)
	{
	case 1:
		document.AskPoint.txtNumber.value = 'TOP100';
		break;
	case 2:
		document.AskPoint.txtNumber.value = 'TOP200';
		break;
	case 3:
		document.AskPoint.txtNumber.value = 'TOP300';
		break;
	}	
	SubmitPrepare();
	document.AskPoint.submit();
}

function ViewBasePoint()
{
	vWH = 268;
	vWW = 643;
	vWN = 'AddNew';
	winDef = 'status=no,resizable=no,scrollbars=yes,toolbar=no,location=no,fullscreen=no,titlebar=yes,height='.concat(vWH).concat(',').concat('width=').concat(vWW).concat(',');
	winDef = winDef.concat('top=').concat((screen.height - vWH)/2).concat(',');
	winDef = winDef.concat('left=').concat((screen.width - vWW)/2);
	newwin = open('', vWN, winDef);
	newwin.focus();

	//document.AskPoint.action = '/Service/DiemthiDH/ViewBasePoint.asp';
	document.AskPoint.action = 'http://member.vnexpress.net/VneService/DiemthiDH/ViewBasePoint.asp';
	document.AskPoint.target = vWN;
	document.AskPoint.submit();
	return true;
}

function SubmitPrepare()
{
	vWH = 275;
	vWW = 643;
	vWN = 'AddNew';
	winDef = 'status=no,resizable=no,scrollbars=yes,toolbar=no,location=no,fullscreen=no,titlebar=yes,height='.concat(vWH).concat(',').concat('width=').concat(vWW).concat(',');
	winDef = winDef.concat('top=').concat((screen.height - vWH)/2).concat(',');
	winDef = winDef.concat('left=').concat((screen.width - vWW)/2);
	newwin = open('', vWN, winDef);
	newwin.focus();
	
	document.AskPoint.action = 'http://member.vnexpress.net/VneService/DiemthiDH/ViewPoint.asp';
	document.AskPoint.target = vWN;
	location.href="http://vnexpress.net/GL/Xa-hoi/Giao-duc/Diem-thi-DH/"
}

function CheckAskPoint()
{
	document.AskPoint.txtNumber.value = Trim(document.AskPoint.txtNumber.value);
	if (document.AskPoint.txtNumber.value == '')
	{
		alert('Hay nhap [So bao danh] hoac [Ho va ten]!');
		return false;
	}

	SubmitPrepare();
	return true;
}

if (document.AskPoint.cboCode.selectedIndex == -1)
{
	document.AskPoint.cboCode.selectedIndex = 0;
}
</script><div class="box-item">
