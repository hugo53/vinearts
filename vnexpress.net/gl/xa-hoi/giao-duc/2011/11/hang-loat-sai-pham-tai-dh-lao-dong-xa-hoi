function writeYear(){
	var curDT = new Date();
	var curYear = curDT.getFullYear();		
	if (curYear > 2009){
		document.write('<select name="cboYear" id="cboYear" onchange="clearChange()">');
		for(var i=curYear; i>=2009; i--){
			document.write('<option value="' + i + '">' + i + '</option>')
		}
		document.write('</select>')
	}
	syear = gmobj('cboYear').value;
}
</script>
