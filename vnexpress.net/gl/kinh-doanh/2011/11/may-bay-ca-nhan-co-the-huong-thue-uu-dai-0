<script>      
var autocompleteTextId = "iptCode";
var tradeCenterIndex = "1";
var suggestionWidth = 250;       
function  ApplyStockAutoComplete(){
	$('#' + autocompleteTextId).flushCache();
	$('#' + autocompleteTextId).autocomplete(stocksymbols[tradeCenterIndex], {
		minChars: 0,
		width: suggestionWidth,
		matchContains: true,
		autoFill: false,
		formatItem: function(row, i, max)
		{
			return row.s + ': ' + row.n;
		},
		formatMatch: function(row, i, max)
		{
			return row.s + ' ' + row.n + ' ' + row.t;
		},
		formatResult: function(row)
		{
			return row.s + '-' + row.n;
		}
	});   
	$('#' + autocompleteTextId).result(function(event, data, formatted) {                             
		document.frmsearch.submit();
		return true;	
	});            
}        
function ChangeFloorCode(index) {
	tradeCenterIndex = index;
	ApplyStockAutoComplete();
}   

ChangeFloorCode(-1);        
</script>
