<script language="javascript">
function ChangeTo(to) {
	if (to == "0")
	{
		if (document.getElementById("tabindex").value = "ns")
		{
			document.getElementById("ns").style.color="rgb(26, 70, 119)";
			document.getElementById("gg").style.color="rgb(129, 134, 174)";
		}
		document.getElementById("tabindex").value = "gg"
		if(document.getElementById("TSearch").value != '' && document.getElementById("TSearch").value != 'Từ khóa tìm kiếm')
		{
			SearchNew();
		}
		else
		{
			alert('Hãy nhập từ khóa tìm kiếm.');
		}		
	}
	if (to == "1")
	{
		if (document.getElementById("tabindex").value = "gg")
		{
			document.getElementById("ns").style.color="rgb(129, 134, 174)";
			document.getElementById("gg").style.color="rgb(26, 70, 119)";
		}
		document.getElementById("tabindex").value = "ns"		
		if(document.getElementById("TSearch").value != '' && document.getElementById("TSearch").value != 'Từ khóa tìm kiếm')
		{
			SearchNew();
		}
		else
		{
			alert('Hãy nhập từ khóa tìm kiếm.');
		}		
	}
}
function SearchNew()
{
	if (document.getElementById("tabindex").value == "gg")
	{
		document.location.target='_blank';
		location.href = "http://www.google.com.vn/#hl=vi&biw=1440&bih=686&q=" + document.getElementById("TSearch").value;
		return false;
	}
	else
	{
		return SearchMe(document.getElementById("TSearch").value, 1);
	}
}
</script>

