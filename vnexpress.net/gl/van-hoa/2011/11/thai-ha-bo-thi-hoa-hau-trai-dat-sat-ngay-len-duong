		<script type="text/javascript">
			cpms_Ap.typ = 2;
			cpms_Ap.ste = 1;
			cpms_Ap.reg = 0;
			cpms_Ap.fol = 51;
			cpms_Ap.dof = 0;
			cpms_Ap.wdt = 300;
			
			cpms_Ap.itf = 3;
			
			cpms_Ap.ShowAd();
		</script>
