<div id="rfloat">
</div>
<script type="text/javascript">
$(function() {
checkPos($(window).width());
$(window).resize(function() {
checkPos($(window).width());
});
});
function checkPos(windowWidth) {
posLeft = (windowWidth - 1000) / 2 - vgads_width - 3;
posRight = (windowWidth - 1000) / 2 - vgads_width + 1;
if (windowWidth < 1250) {
$('#rfloat').hide();
} else {
$('#rfloat').show();
$("#rfloat").css({ top: 5, right: posRight, position: "fixed", top: 2 });
}
}
</script>
