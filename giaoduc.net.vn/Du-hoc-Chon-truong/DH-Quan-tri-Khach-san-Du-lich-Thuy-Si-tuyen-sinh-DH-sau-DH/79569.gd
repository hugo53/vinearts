var isIE = (navigator.appVersion.indexOf("MSIE") != -1) ? true : false;
var windowWidth = $("body").width();
var vgads_domain = 10;
var vgads_category = 0;
var vgads_type = 0;
var vgads_show = 0;
var vgads_no_price = 0;
var vgads_width = 120;
var vgads_quantity = 3;
