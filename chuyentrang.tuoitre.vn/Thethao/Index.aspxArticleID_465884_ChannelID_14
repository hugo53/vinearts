        try {
            document.getElementById("mnu_2").className = "QvActive";
            document.getElementById("mnu_0").className = "tt";
        }
        catch (err) { }
    }
    else if (url.toString().toLowerCase().match(/dien-kinh-boi-loi/)) {
        try {
