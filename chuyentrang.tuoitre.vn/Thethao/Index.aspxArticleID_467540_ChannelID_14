<style type="text/css">
    #divContent
    {
        font-family: 'arial'; 
        font-size: 12px; 
        color: #000;  
    }
    .pager {clear: both; width: 95%; text-align: center;padding: 12px 0px 5px 0px; font-size: 14px; }
    .pager li{display: inline-block; }
    .pager a{color:#777;text-decoration: none;display: inline-block; border: 1px solid #d8d8d8; padding: 0px 5px; margin-right: 5px; }
    .pager a.select{color:#0070a0;font-weight: bold;border: none;}
    .pager a.select:hover{border: none;}
    .pager a.none{border: none;}
