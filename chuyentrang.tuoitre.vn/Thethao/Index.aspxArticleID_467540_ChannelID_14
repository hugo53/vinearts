            <input id="txtKeyword" type="text" name="SearchQuery" onkeypress="return trapEnterKey('chkYahoo',this.value,event);" class="inputSearch floatLeft" style="width:175px; height:16px;" value="Tìm thông tin trên Tuổi Trẻ"  onblur="if(this.value==''){this.value='Tìm thông tin trên Tuổi Trẻ';}" onfocus="if(this.value=='Tìm thông tin trên Tuổi Trẻ'){this.value='';}"/>
            <input type="button" class="inputSearch floatLeft" onclick="searchEngine();" alt="Tìm Kiếm" id="go_search_yahoo" style="width:60px; height:22px; cursor:pointer" />
