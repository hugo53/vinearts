        <div class="itemBoxKqBda">
            
                    <div class="titleItemKqTeam">
                        <div class="pad8">
                            Lượt 4 vòng bảng Champions League</div>
                    </div>
                    <ul class="mnuItemKq">
                        
                                <li><a href="#">
                                    <div class="floatLeft textLeft" style="width: 35%">
                                        Villarreal
                                    </div>
