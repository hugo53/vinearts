<script language="javascript" type="text/javascript">
    var url = window.location.href;
    Items = new Array("mnu_0", "mnu_1", "mnu_2", "mnu_3", "mnu_4", "mnu_5");
    if (url.toString().toLowerCase().match(/Trang-chu/)) {
        try {
            document.getElementById("mnu_0").className = "ttActive";
        }
        catch (err) { }
    }
    else if (url.toString().toLowerCase().match(/bong-da/)) {
        try {
            document.getElementById("mnu_1").className = "bdActive";
            document.getElementById("mnu_0").className = "tt";
        }
        catch (err) { }
    }
    else if (url.toString().toLowerCase().match(/quan-vot/)) {
        try {
            document.getElementById("mnu_2").className = "QvActive";
            document.getElementById("mnu_0").className = "tt";
        }
        catch (err) { }
    }
    else if (url.toString().toLowerCase().match(/dien-kinh-boi-loi/)) {
        try {
            document.getElementById("mnu_3").className = "nmalActive";
            document.getElementById("mnu_0").className = "tt";
        }
        catch (err) { }
    }
    else if (url.toString().toLowerCase().match(/su-kien-binh-luan/)) {
        try {
            document.getElementById("mnu_4").className = "nmalActive";
            document.getElementById("mnu_0").className = "tt";
        }
        catch (err) { }
    }
    else if (url.toString().toLowerCase().match(/cac-mon-khac/)) {
        try {
            document.getElementById("mnu_5").className = "nmalActive";
            document.getElementById("mnu_0").className = "tt";
        }
        catch (err) { }
    }  
    else {
        try {
            document.getElementById("mnu_0").className = "ttActive";            
        }
        catch (err) { }
    } 
</script>
<script language="javascript" type="text/javascript">
    function setHomepage()
    {
        if (document.all)
        {
            document.body.style.behavior = 'url(#default#homepage)';
            document.body.setHomePage('http://Thethao.tuoitre.vn/Index.html');
        }
        else {
            alert("Kéo Logo Tuổi trẻ vào nút Home bên cạnh thanh địa chỉ để đặt làm trang chủ");
        }
    }  
</script>
        
