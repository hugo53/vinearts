    var url = window.location.href;
    Items = new Array("mnu_0", "mnu_1", "mnu_2", "mnu_3", "mnu_4", "mnu_5");
    if (url.toString().toLowerCase().match(/Trang-chu/)) {
        try {
            document.getElementById("mnu_0").className = "ttActive";
        }
        catch (err) { }
    }
    else if (url.toString().toLowerCase().match(/bong-da/)) {
