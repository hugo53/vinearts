    function Tab6(showAtiID, showID1, showID2, showID3, hideID4, hideID5) {
        // show elment
        document.getElementById(showAtiID).className = "tabAt";
        // document.getElementById(showAtiID + "Content").style.display = "block";
        // hide element

        document.getElementById(showID1).style.display = "block";
        document.getElementById(showID1).className = "Tab_left";


        // show  subTab active
        document.getElementById(showID2).style.display = "block";
        document.getElementById(showID2).className = "actabKq color3";
        document.getElementById(showID2 + "Content").style.display = "block";

        // show Tab but hile Content
        document.getElementById(showID3).style.display = "block";
        document.getElementById(showID3).className = "color3";
        document.getElementById(showID3 + "Content").style.display = "none";


        //hide Tab and content
        document.getElementById(hideID4).style.display = "none";
        document.getElementById(hideID4 + "Content").style.display = "none";

        document.getElementById(hideID5).style.display = "none";
        document.getElementById(hideID5 + "Content").style.display = "none";


    } 
    
    
        
</script>

