    <div class="pad8"> 
        <div class="padBt10">	
   	        <div class="padBt10">
        	  <div class="floatLeft bold"><a class="color5" href="http://thethao.tuoitre.vn/The-thao/Index.html">Thể thao</a></div>
              <div class="floatRight color6"><script type="text/javascript">getTimeString('2011/11/08 00:10:29');</script></div>                    
