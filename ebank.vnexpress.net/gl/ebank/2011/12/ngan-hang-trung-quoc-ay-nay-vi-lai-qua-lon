	<script type="text/javascript" language="javascript">
		var tid = 1;
		function activeTab(i){
			tabid = i;
			orderby = -1;
			ordertype = -1;
			for(var j=1; j<4; j++){
				if(i==j){
					gmobj('tab_Type_' + j).className = 'tagAct';
					showInterestRateNew(i,0,-1);
					gmobj('dInterestRate').style.display = '';									
				}
				else{
					gmobj('tab_Type_' + j).className = 'tagNor';
				}
			}
		}
		showInterestRateNew(1,0,'-1');
		
		function ShowInterestRate(){
			showInterestRateNew(tid,0,-1);
		}
	</script>
