<script type="text/javascript" language="javascript">
	function showATM(i){			
		var sLink = '';
		sLink = '/Extend/ATM/View.asp?id=' + i;
		AjaxRequest.get({
			'url':sLink,
			'onSuccess':function(req){
				gmobj('dResult').style.display = 'block';
				var strHeader = '';
				strHeader += '<thead class="thead-atm">'
				strHeader += '	<td width="26%" class="atm-td1">Tỉnh thành</td><td width="74%" class="atm-td2">Địa chỉ</td>';
        strHeader += '</thead>';
				strHeader += req.responseText;
				
				strHeader = strHeader.replace(/<li class="(STT|timeATM|qtyATM)">.*<\/li>/gi, '');
				strHeader = strHeader.replace(/<li class="placeATM align-left">(.*)<\/li>/gi, '<td class="atm-td1">\$1</td>');
				strHeader = strHeader.replace(/<li class="addresATM align-left">(.*)<\/li>/gi, '<td class="atm-td2">\$1</td>');				
				strHeader = strHeader.replace(/<ul class="ulATM odd">/gi, '<tr class="atm-tr1">');
				strHeader = strHeader.replace(/<ul class="ulATM event">/gi, '<tr class="atm-tr2">');
				strHeader = strHeader.replace(/<\/ul>/gi, '</tr>');
				gmobj('dResult').innerHTML = '<table cellspacing="1" cellpadding="0" border="1px" class="tbl-atm">' + strHeader + '</table>';
			},
			'onError':function(req){
				gmobj('dResult').style.display = 'none';
				gmobj('dResult').innerHTML = '';				
			}
		})
	}
	
	function demoReplaceClick() {
		var re = new RegExp(document.demoMatch.regex.value, "g");
		document.demoMatch.result.value = 
			document.demoMatch.subject.value.replace(re, 
				document.demoMatch.replacement.value);
	}

	//showATM(3);
</script>
