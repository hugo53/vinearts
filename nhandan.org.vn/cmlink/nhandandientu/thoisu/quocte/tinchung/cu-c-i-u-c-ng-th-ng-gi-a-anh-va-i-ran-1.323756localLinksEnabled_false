      submitHandler: function(form) {
        jQuery("#submitCommentBlogPost-278318").css("background-color", "#ccc");
        jQuery("#submitCommentBlogPost-278318").attr("disabled", true);
        
        form.submit();
        		alert("Cảm ơn Quý vị đã đóng góp ý kiến về bài viết, chúng tôi đã nhận được ý kiến của Quý vị");
		      }
    };

function renderCaptcha278318() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-278318");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-278318');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-278318" />');
        jQuery("#captcha_image-278318").click(renderCaptcha278318);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-278318").click(renderCaptcha278318);
    
    jQuery("#comment_this_article-278318").click(function() {
        firstTimeCaptcha278318();
        jQuery("#comments_container-278318").slideToggle("fast");
        jQuery("#comment_author-278318").focus();
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-278318").validate(validation);
  });
  
</script>
</div>

