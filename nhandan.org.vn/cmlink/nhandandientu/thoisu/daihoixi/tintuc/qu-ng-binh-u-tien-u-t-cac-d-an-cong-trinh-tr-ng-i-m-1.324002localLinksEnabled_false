$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=Quảng Bình ưu tiên đầu tư các dự án, công trình trọng điểm&url=http://nhandan.org.vn/cmlink/nhandandientu/thoisu/daihoixi/tintuc/qu-ng-binh-u-tien-u-t-cac-d-an-cong-trinh-tr-ng-i-m-1.324002&locale=vi_VN");
