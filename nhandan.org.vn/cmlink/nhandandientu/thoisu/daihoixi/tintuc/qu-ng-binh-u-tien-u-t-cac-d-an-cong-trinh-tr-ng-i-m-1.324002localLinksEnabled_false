function renderCaptcha278610() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-278610");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-278610');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-278610" />');
        jQuery("#captcha_image-278610").click(renderCaptcha278610);
    }
}

