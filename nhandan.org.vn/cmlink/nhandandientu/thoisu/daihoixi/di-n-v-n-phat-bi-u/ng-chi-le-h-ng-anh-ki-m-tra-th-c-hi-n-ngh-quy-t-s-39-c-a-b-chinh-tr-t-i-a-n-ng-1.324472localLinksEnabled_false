function renderCaptcha279175() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-279175");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-279175');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-279175" />');
        jQuery("#captcha_image-279175").click(renderCaptcha279175);
    }
}

