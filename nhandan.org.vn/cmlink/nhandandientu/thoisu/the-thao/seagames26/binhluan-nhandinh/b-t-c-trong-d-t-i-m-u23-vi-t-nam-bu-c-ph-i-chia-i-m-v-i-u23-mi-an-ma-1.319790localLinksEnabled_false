function sendMail(){
$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=Bế tắc trong dứt điểm, U23 Việt Nam buộc phải chia điểm với U23 Mi-an-ma&url=http://nhandan.org.vn/cmlink/nhandandientu/thoisu/the-thao/seagames26/binhluan-nhandinh/b-t-c-trong-d-t-i-m-u23-vi-t-nam-bu-c-ph-i-chia-i-m-v-i-u23-mi-an-ma-1.319790&locale=vi_VN");
