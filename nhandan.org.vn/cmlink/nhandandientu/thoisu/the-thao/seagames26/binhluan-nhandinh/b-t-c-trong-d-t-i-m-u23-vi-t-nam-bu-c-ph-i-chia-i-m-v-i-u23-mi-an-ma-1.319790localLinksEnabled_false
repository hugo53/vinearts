function renderCaptcha273725() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-273725");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-273725');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-273725" />');
        jQuery("#captcha_image-273725").click(renderCaptcha273725);
    }
}

