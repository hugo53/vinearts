function renderCaptcha275542() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-275542");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-275542');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-275542" />');
        jQuery("#captcha_image-275542").click(renderCaptcha275542);
    }
}

jQuery().ready(function() { 
            
    
