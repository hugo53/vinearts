function renderCaptcha273164() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-273164");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-273164');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-273164" />');
        jQuery("#captcha_image-273164").click(renderCaptcha273164);
    }
}

