function renderCaptcha276706() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-276706");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-276706');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-276706" />');
        jQuery("#captcha_image-276706").click(renderCaptcha276706);
    }
}

