function renderCaptcha275828() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-275828");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-275828');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-275828" />');
        jQuery("#captcha_image-275828").click(renderCaptcha275828);
    }
}

