$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=Thất vọng cầu lông Việt Nam&url=http://nhandan.org.vn/cmlink/nhandandientu/thoisu/the-thao/seagames26/benle/th-t-v-ng-c-u-long-vi-t-nam-1.321622&locale=vi_VN");
