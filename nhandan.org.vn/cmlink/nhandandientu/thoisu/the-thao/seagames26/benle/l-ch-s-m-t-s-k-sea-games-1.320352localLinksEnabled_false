function renderCaptcha274364() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-274364");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-274364');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-274364" />');
        jQuery("#captcha_image-274364").click(renderCaptcha274364);
    }
}

