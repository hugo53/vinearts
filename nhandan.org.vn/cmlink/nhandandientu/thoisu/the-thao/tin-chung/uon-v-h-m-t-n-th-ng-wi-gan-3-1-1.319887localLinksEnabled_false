function renderCaptcha273831() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-273831");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-273831');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-273831" />');
        jQuery("#captcha_image-273831").click(renderCaptcha273831);
    }
}

