function renderCaptcha278476() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-278476");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-278476');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-278476" />');
        jQuery("#captcha_image-278476").click(renderCaptcha278476);
    }
}

