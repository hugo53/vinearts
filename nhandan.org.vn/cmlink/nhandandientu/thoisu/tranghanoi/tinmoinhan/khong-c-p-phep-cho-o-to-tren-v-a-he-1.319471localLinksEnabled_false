function renderCaptcha273352() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-273352");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-273352');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-273352" />');
        jQuery("#captcha_image-273352").click(renderCaptcha273352);
    }
}

jQuery().ready(function() { 
            
    
