function sendMail(){
$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=Không cấp phép cho ô-tô đỗ trên vỉa hè&url=http://nhandan.org.vn/cmlink/nhandandientu/thoisu/tranghanoi/tinmoinhan/khong-c-p-phep-cho-o-to-tren-v-a-he-1.319471&locale=vi_VN");
