function renderCaptcha277033() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-277033");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-277033');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-277033" />');
        jQuery("#captcha_image-277033").click(renderCaptcha277033);
    }
}

