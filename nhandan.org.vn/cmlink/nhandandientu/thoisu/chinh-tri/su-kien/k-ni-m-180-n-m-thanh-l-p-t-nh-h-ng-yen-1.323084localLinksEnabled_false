    };

function renderCaptcha277561() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-277561");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-277561');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-277561" />');
        jQuery("#captcha_image-277561").click(renderCaptcha277561);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-277561").click(renderCaptcha277561);
    
