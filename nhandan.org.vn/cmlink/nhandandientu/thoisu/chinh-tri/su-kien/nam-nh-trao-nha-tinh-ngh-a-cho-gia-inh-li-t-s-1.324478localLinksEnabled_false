function renderCaptcha279183() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-279183");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-279183');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-279183" />');
        jQuery("#captcha_image-279183").click(renderCaptcha279183);
    }
}

