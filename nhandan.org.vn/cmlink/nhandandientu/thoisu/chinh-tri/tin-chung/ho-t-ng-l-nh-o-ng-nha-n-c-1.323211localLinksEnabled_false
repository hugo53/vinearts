function renderCaptcha277677() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-277677");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-277677');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-277677" />');
        jQuery("#captcha_image-277677").click(renderCaptcha277677);
    }
}

