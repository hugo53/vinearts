		      }
    };

function renderCaptcha278083() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-278083");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-278083');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-278083" />');
        jQuery("#captcha_image-278083").click(renderCaptcha278083);
    }
}

jQuery().ready(function() { 
            
    
