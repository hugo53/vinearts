		      }
    };

function renderCaptcha274583() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-274583");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-274583');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-274583" />');
        jQuery("#captcha_image-274583").click(renderCaptcha274583);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-274583").click(renderCaptcha274583);
    
    jQuery("#comment_this_article-274583").click(function() {
        firstTimeCaptcha274583();
        jQuery("#comments_container-274583").slideToggle("fast");
        jQuery("#comment_author-274583").focus();
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-274583").validate(validation);
  });
  
</script>
