function renderCaptcha273837() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-273837");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-273837');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-273837" />');
        jQuery("#captcha_image-273837").click(renderCaptcha273837);
    }
}

