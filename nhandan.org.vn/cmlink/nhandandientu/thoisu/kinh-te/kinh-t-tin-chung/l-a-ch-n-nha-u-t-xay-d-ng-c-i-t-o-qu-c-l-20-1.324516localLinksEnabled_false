function renderCaptcha279249() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-279249");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-279249');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-279249" />');
        jQuery("#captcha_image-279249").click(renderCaptcha279249);
    }
