<script language="javascript" type="text/javascript">
    function SearchOnBlur(field)
    {
    	if(field.value==''){ field.value='Tìm bài tin ';}
    }

    function SearchOnFocus(field)
    {
    	if(field.value=='Tìm bài tin '){ field.value = ''; }
    }
	
	
    function verifySearchField()
    {
    	if(document.getElementById("q").value=='Tìm bài tin '){ document.getElementById("q").value = ''; }
    }
	
	function submit(){
		$('#advancesearch').submit("/cmlink/customsearchplugin-7.232160");
		return false;	
	}

</script>























