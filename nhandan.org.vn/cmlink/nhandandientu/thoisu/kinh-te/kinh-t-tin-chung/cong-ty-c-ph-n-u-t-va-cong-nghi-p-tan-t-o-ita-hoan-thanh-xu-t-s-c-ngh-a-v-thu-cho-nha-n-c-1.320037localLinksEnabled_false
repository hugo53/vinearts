    };

function renderCaptcha274017() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-274017");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-274017');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-274017" />');
        jQuery("#captcha_image-274017").click(renderCaptcha274017);
    }
}

