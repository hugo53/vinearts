function sendMail(){
$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=Thương hiệu nông sản: Bảo vệ, mất và lấy lại&url=http://nhandan.org.vn/cmlink/nhandandientu/thoisu/kinh-te/th-ng-hi-u/th-ng-hi-u-nong-s-n-b-o-v-m-t-va-l-y-l-i-1.320305&locale=vi_VN");
