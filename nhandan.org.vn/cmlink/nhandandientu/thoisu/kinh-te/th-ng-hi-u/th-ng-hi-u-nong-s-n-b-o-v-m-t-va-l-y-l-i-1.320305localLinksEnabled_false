        },
		        comment_email: {
          required: "Bạn chưa nhập email hoặc email không đúng. Xin hãy nhập lại.",
          email: "Bạn chưa nhập email hoặc email không đúng. Xin hãy nhập lại."
        },
        captcha_title: {
          required: "Xin hãy nhập Tiêu đề."
        }
      },
      submitHandler: function(form) {
        jQuery("#submitCommentBlogPost-274310").css("background-color", "#ccc");
        jQuery("#submitCommentBlogPost-274310").attr("disabled", true);
        
        form.submit();
        		alert("Cảm ơn Quý vị đã đóng góp ý kiến về bài viết, chúng tôi đã nhận được ý kiến của Quý vị");
		      }
    };

function renderCaptcha274310() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-274310");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-274310');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-274310" />');
        jQuery("#captcha_image-274310").click(renderCaptcha274310);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-274310").click(renderCaptcha274310);
    
    jQuery("#comment_this_article-274310").click(function() {
        firstTimeCaptcha274310();
        jQuery("#comments_container-274310").slideToggle("fast");
        jQuery("#comment_author-274310").focus();
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-274310").validate(validation);
  });
  
</script>
