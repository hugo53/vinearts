    };

function renderCaptcha274110() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-274110");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-274110');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-274110" />');
        jQuery("#captcha_image-274110").click(renderCaptcha274110);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-274110").click(renderCaptcha274110);
    
    jQuery("#comment_this_article-274110").click(function() {
        firstTimeCaptcha274110();
        jQuery("#comments_container-274110").slideToggle("fast");
        jQuery("#comment_author-274110").focus();
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-274110").validate(validation);
  });
  
