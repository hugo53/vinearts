function renderCaptcha278740() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-278740");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-278740');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-278740" />');
        jQuery("#captcha_image-278740").click(renderCaptcha278740);
    }
}

jQuery().ready(function() { 
            
    
