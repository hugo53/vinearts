      submitHandler: function(form) {
        jQuery("#submitCommentBlogPost-275028").css("background-color", "#ccc");
        jQuery("#submitCommentBlogPost-275028").attr("disabled", true);
        
        form.submit();
        		alert("Cảm ơn Quý vị đã đóng góp ý kiến về bài viết, chúng tôi đã nhận được ý kiến của Quý vị");
		      }
    };

function renderCaptcha275028() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-275028");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-275028');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-275028" />');
        jQuery("#captcha_image-275028").click(renderCaptcha275028);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-275028").click(renderCaptcha275028);
    
    jQuery("#comment_this_article-275028").click(function() {
        firstTimeCaptcha275028();
        jQuery("#comments_container-275028").slideToggle("fast");
        jQuery("#comment_author-275028").focus();
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-275028").validate(validation);
  });
  
</script>
</div>

  <div id="commentsField-275028" style="display: none;"></div>
