function renderCaptcha277745() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-277745");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-277745');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-277745" />');
        jQuery("#captcha_image-277745").click(renderCaptcha277745);
    }
}

