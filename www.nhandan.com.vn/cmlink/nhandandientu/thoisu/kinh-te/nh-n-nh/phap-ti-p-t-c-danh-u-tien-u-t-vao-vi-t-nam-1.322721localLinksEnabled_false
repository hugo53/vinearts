function sendMail(){
$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=Pháp tiếp tục dành ưu tiên đầu tư vào Việt Nam&url=http://www.nhandan.com.vn/cmlink/nhandandientu/thoisu/kinh-te/nh-n-nh/phap-ti-p-t-c-danh-u-tien-u-t-vao-vi-t-nam-1.322721&locale=vi_VN");
