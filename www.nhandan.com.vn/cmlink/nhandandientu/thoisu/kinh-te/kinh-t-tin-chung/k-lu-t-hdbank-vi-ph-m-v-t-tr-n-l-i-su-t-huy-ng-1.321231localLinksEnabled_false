function renderCaptcha275352() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-275352");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-275352');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-275352" />');
        jQuery("#captcha_image-275352").click(renderCaptcha275352);
    }
}

