function renderCaptcha276496() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-276496");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-276496');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-276496" />');
        jQuery("#captcha_image-276496").click(renderCaptcha276496);
    }
}

jQuery().ready(function() { 
            
    
