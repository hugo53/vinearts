function renderCaptcha274005() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-274005");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-274005');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-274005" />');
        jQuery("#captcha_image-274005").click(renderCaptcha274005);
    }
}

