function sendMail(){
$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=Chủ tịch nước Trương Tấn Sang tiếp; Thủ tướng Nguyễn Tấn Dũng đón, hội đàm; Chủ tịch Quốc hội Nguyễn Sinh Hùng tiếp Thủ tướng Cộng hòa E-xtô-ni-a A.An-síp&url=http://www.nhandan.com.vn/cmlink/nhandandientu/thoisu/chinh-tri/su-kien/ch-t-ch-n-c-tr-ng-t-n-sang-ti-p-th-t-ng-nguy-n-t-n-d-ng-on-h-i-am-ch-t-ch-qu-c-h-i-nguy-n-sinh-hung-ti-p-th-t-ng-c-ng-hoa-e-xto-ni-a-a-an-sip-1.320027&locale=vi_VN");
