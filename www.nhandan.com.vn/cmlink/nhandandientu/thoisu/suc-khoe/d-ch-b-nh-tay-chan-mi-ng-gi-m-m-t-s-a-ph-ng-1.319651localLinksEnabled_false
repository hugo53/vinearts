		      }
    };

function renderCaptcha273545() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-273545");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-273545');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-273545" />');
        jQuery("#captcha_image-273545").click(renderCaptcha273545);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-273545").click(renderCaptcha273545);
    
    jQuery("#comment_this_article-273545").click(function() {
        firstTimeCaptcha273545();
        jQuery("#comments_container-273545").slideToggle("fast");
        jQuery("#comment_author-273545").focus();
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-273545").validate(validation);
  });
  
