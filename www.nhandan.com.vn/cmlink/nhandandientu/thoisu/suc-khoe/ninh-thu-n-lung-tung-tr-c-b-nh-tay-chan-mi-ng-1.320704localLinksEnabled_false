      submitHandler: function(form) {
        jQuery("#submitCommentBlogPost-274783").css("background-color", "#ccc");
        jQuery("#submitCommentBlogPost-274783").attr("disabled", true);
        
        form.submit();
        		alert("Cảm ơn Quý vị đã đóng góp ý kiến về bài viết, chúng tôi đã nhận được ý kiến của Quý vị");
		      }
    };

function renderCaptcha274783() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-274783");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-274783');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-274783" />');
        jQuery("#captcha_image-274783").click(renderCaptcha274783);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-274783").click(renderCaptcha274783);
    
    jQuery("#comment_this_article-274783").click(function() {
        firstTimeCaptcha274783();
        jQuery("#comments_container-274783").slideToggle("fast");
        jQuery("#comment_author-274783").focus();
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-274783").validate(validation);
  });
  
</script>
</div>

