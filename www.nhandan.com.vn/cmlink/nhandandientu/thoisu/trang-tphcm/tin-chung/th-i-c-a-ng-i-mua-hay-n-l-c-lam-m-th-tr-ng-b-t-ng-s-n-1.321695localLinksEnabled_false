function renderCaptcha275933() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-275933");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-275933');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-275933" />');
        jQuery("#captcha_image-275933").click(renderCaptcha275933);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-275933").click(renderCaptcha275933);
    
    jQuery("#comment_this_article-275933").click(function() {
        firstTimeCaptcha275933();
        jQuery("#comments_container-275933").slideToggle("fast");
        jQuery("#comment_author-275933").focus();
    });
    
});

