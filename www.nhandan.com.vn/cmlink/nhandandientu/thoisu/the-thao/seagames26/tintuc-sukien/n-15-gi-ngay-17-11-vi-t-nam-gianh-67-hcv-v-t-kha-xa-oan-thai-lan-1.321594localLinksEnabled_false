function renderCaptcha275794() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-275794");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-275794');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-275794" />');
        jQuery("#captcha_image-275794").click(renderCaptcha275794);
    }
}

