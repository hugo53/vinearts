function renderCaptcha275768() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-275768");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-275768');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-275768" />');
        jQuery("#captcha_image-275768").click(renderCaptcha275768);
    }
}

