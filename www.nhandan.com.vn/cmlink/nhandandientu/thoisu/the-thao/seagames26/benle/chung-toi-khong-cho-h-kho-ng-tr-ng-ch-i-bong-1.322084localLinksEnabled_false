function sendMail(){
$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=Chúng tôi không cho họ khoảng trống để chơi bóng&url=http://www.nhandan.com.vn/cmlink/nhandandientu/thoisu/the-thao/seagames26/benle/chung-toi-khong-cho-h-kho-ng-tr-ng-ch-i-bong-1.322084&locale=vi_VN");
