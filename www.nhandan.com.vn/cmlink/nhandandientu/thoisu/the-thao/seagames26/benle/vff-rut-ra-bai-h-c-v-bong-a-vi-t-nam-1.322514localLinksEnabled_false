function sendMail(){
$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=VFF rút ra bài học về bóng đá Việt Nam&url=http://www.nhandan.com.vn/cmlink/nhandandientu/thoisu/the-thao/seagames26/benle/vff-rut-ra-bai-h-c-v-bong-a-vi-t-nam-1.322514&locale=vi_VN");
