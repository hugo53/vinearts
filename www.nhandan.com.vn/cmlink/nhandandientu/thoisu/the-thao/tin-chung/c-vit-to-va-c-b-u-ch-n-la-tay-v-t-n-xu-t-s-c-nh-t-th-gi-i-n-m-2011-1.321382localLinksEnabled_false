function renderCaptcha275536() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-275536");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-275536');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-275536" />');
        jQuery("#captcha_image-275536").click(renderCaptcha275536);
    }
}

