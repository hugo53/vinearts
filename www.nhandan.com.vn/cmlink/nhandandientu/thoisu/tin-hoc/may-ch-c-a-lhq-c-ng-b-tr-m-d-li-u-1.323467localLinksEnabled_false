<script>

JNewQuery().ready(function() { 
    JNewQuery("#email_this_article-123").click(function() {
        sendMail();
    });
	
});


function sendMail(){
$j('.sendMail').sendMail("/cmlink/mail-configuration-17.123?sendMail=init&title=Máy chủ của LHQ cũng bị trộm dữ liệu&url=http://www.nhandan.com.vn/cmlink/nhandandientu/thoisu/tin-hoc/may-ch-c-a-lhq-c-ng-b-tr-m-d-li-u-1.323467&locale=vi_VN");
return false;
}

JNewQuery.fn.sendMail = function(url)
    {
    if(url == null) 
    return;
    
    window.open(url,"_blank","Send article through email","location=0,status=0,scrollbars=0,width=300,height=400"); 
    }
