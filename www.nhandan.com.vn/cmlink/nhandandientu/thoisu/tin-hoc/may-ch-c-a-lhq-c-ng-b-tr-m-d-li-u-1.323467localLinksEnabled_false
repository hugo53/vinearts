function renderCaptcha277997() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-277997");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-277997');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-277997" />');
        jQuery("#captcha_image-277997").click(renderCaptcha277997);
    }
