		      }
    };

function renderCaptcha274526() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-274526");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-274526');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-274526" />');
        jQuery("#captcha_image-274526").click(renderCaptcha274526);
    }
}

jQuery().ready(function() { 
            
    
