		      }
    };

function renderCaptcha278554() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-278554");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-278554');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-278554" />');
        jQuery("#captcha_image-278554").click(renderCaptcha278554);
    }
}

jQuery().ready(function() { 
            
    
    //jQuery("#captcha_container-278554").click(renderCaptcha278554);
    
    jQuery("#comment_this_article-278554").click(function() {
        firstTimeCaptcha278554();
        jQuery("#comments_container-278554").slideToggle("fast");
        jQuery("#comment_author-278554").focus();
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-278554").validate(validation);
  });
  
