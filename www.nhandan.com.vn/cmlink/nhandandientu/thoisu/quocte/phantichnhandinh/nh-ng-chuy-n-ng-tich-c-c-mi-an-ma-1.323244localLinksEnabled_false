function renderCaptcha277721() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-277721");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-277721');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-277721" />');
        jQuery("#captcha_image-277721").click(renderCaptcha277721);
    }
}

jQuery().ready(function() { 
            
    
