<script language="javascript" type="text/javascript">
    function ChangeStatus(id)
    {
        if(id == 'NewProjectsLink') // Click mouse on NewProjects tab
        {   
            document.getElementById('NewProjectsLink').style.backgroundImage = 'url(Images/CenterTabAct.gif)';
            document.getElementById('SelfIntroducedProjectsLink').style.backgroundImage = 'url(Images/CenterTab.gif)';
            document.getElementById('SelfIntroducedProjects').style.display = 'none';
            document.getElementById('NewProjects').style.display = 'block';
        }
        else // Click mouse on SelfIntroducedProjects tab
        {
            document.getElementById('SelfIntroducedProjectsLink').style.backgroundImage = 'url(Images/CenterTabAct.gif)';
            document.getElementById('NewProjectsLink').style.backgroundImage = 'url(Images/CenterTab.gif)';
            document.getElementById('NewProjects').style.display = 'none';
            document.getElementById('SelfIntroducedProjects').style.display = 'block';
        }
    }
</script>
