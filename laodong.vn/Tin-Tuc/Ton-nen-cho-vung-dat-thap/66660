        function EmailPopup() {
            var objShortName = document.getElementById('hdShortName').value;
            var objId = document.getElementById('hdId').value;
            window.open('/Email/' + objShortName + '/' + objId, '', 'width = 650,height = 480,location= yes, resizable=yes,scrollbars=yes, toolbar=no,location=no,menubar=no');
        }

        function InAnPopup() {
            var objShortName = document.getElementById('hdShortName').value;
            var objId = document.getElementById('hdId').value;
            window.open('/In-an/' + objShortName + '/' + objId, '', 'width = 890,height = 480,location= yes, resizable=yes,scrollbars=yes, toolbar=no,location=no,menubar=no');
        }
        
    </script>
