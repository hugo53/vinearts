<!-- Place this tag where you want the +1 button to render -->
    <script language="javascript" type="text/javascript">
        socialshare('http://laodong.com.vn/Tin-Tuc/3-ky-luc-xac-lap-tai-Lien-hoan-Tra-quoc-te/66607', '3 kỷ lục xác lập tại Liên hoan Trà quốc tế', '3 kỷ lục xác lập tại Liên hoan Trà quốc tế', 'sociable', 1000443459)

        function EmailPopup() {
            var objShortName = document.getElementById('hdShortName').value;
            var objId = document.getElementById('hdId').value;
            window.open('/Email/' + objShortName + '/' + objId, '', 'width = 650,height = 480,location= yes, resizable=yes,scrollbars=yes, toolbar=no,location=no,menubar=no');
        }

        function InAnPopup() {
