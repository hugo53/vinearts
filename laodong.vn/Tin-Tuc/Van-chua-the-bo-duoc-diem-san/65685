<div class="BoxCenter">
    <script type="text/javascript">
<!--    // <![CDATA[
    function DocNhieuNhat() {
        document.getElementById('divBaiMoiNhat').style.display = 'none';
        document.getElementById('idBaiMoiNhat').className = '';

        document.getElementById('divDocNhieuNhat').style.display = 'block';
        document.getElementById('idDocNhieuNhat').className = 'active';
    }

    function BaiMoiNhat() {
        document.getElementById('divBaiMoiNhat').style.display = 'block';
        document.getElementById('idBaiMoiNhat').className = 'active';

        document.getElementById('divDocNhieuNhat').style.display = 'none';
        document.getElementById('idDocNhieuNhat').className = '';
    }
// ]]> -->    
</script>

<div class="Box">
